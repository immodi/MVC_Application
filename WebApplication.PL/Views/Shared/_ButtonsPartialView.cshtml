@model string 

<td class="pt-3">
    <a asp-action="Edit" class="btn btn-dark" asp-route-id="@Model">
        <i class="fas fa-pencil"></i> Edit
    </a> 
</td>
<td class="pt-3">
    <a asp-action="Details" class="btn btn-light" asp-route-id="@Model">
        <i class="fas fa-eye"></i> Details
    </a> 
</td>
<td class="pt-3">
    <a asp-action="Delete" class="btn btn-danger" asp-route-id="@Model">
        <i class="fas fa-trash"></i> Delete
    </a>
</td>